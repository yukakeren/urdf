   </link>
   <joint name="shoulder" type="revolute">
       <axis xyz="0 1 0"/>
       <limit velocity="1.0" effort="30.0" upper="0.6981" lower="-1.5708"/>
       <parent link="torso"/>
       <child link="upper_arm"/>
       <origin rpy="0 1.5708 0" xyz="0.02 0.06 0.06"/>
   </joint>
   <link name="forearm">
       <inertial>
           <mass value="1.0"/>
