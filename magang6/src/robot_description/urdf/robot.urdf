<robot name="arm">
   <link name="world"/>
   <link name="base_link">
       <inertial>
           <mass value="1.0"/>
           <origin rpy="0 0 0" xyz="0 0 0.025"/>
           <inertia ixx="0.0027" iyy="0.0027" izz="0.005" ixy="0" ixz="0" iyz="0"/>
       </inertial>
       <visual>
           <geometry>
               <box size="0.1 0.1 0.1"/>
           </geometry>
           <material name="silver">
               <color rgba="0.75 0.75 0.75 1"/>
           </material>
           <origin rpy="0 0 0" xyz="0 0 0.025"/>
       </visual>
       <collision>
           <geometry>
               <box size="0.1 0.1 0.1"/>
           </geometry>
           <origin rpy="0 0 0" xyz="0 0 0.025"/>
       </collision>
   </link>
   <joint name="fixed" type="fixed">
       <parent link="world"/>
       <child link="base_link"/>
   </joint>
   <link name="torso">
       <inertial>
           <mass value="1.0"/>
           <origin rpy="0 0 0" xyz="0 0 0"/>
           <inertia ixx="0.02146" iyy="0.02146" izz="0.00125" ixy="0" ixz="0" iyz="0"/>
       </inertial>

       <visual>       
           <geometry>
               <cylinder length="0.12" radius="0.03"/>
           </geometry>
           <material name="silver">
               <color rgba="0.75 0.75 0.75 1"/>
           </material>
           <origin rpy="0 0 0" xyz="0 0 0"/>
       </visual>
       <collision>
           <geometry>
               <cylinder length="0.12" radius="0.03"/>
           </geometry>
           <origin rpy="0 0 0" xyz="0 0 0"/>
       </collision>
   </link>
   <joint name="hip" type="revolute">
       <axis xyz="0 0 1"/>
 <limit velocity="1.0" effort="30.0" upper="1.5708" lower="-1.5708"/>
       <parent link="base_link"/>
       <child link="torso"/>
       <origin rpy="0 0 0" xyz="0 0 0.11"/>
   </joint>
   <link name="upper_arm">
       <inertial>
           <mass value="1.0"/>
           <origin rpy="0 0 0" xyz="0 0 0.05"/>
           <inertia ixx="0.01396" iyy="0.01396" izz="0.00125" ixy="0" ixz="0" iyz="0"/>
       </inertial>
       <visual>
           <geometry>
               <cylinder length="0.3" radius="0.03"/>
           </geometry>
           <material name="silver"/>
           <origin rpy="0 0 0" xyz="0 0 0.05"/>
       </visual>
       <collision>
           <geometry>
               <cylinder length="0.3" radius="0.03"/>
           </geometry>
           <origin rpy="0 0 0" xyz="0 0 0.05"/>
