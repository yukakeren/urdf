       <collision>
           <geometry>
               <cylinder length="0.1" radius="0.03"/>
           </geometry>
           <origin rpy="0 0 0" xyz="0.1 0 0.2"/>
       </collision>
   </link>
      <joint name="coba" type="revolute">
       <axis xyz="0 0 0"/>
       <limit velocity="1.0" effort="30.0" upper="1.5708" lower="-1.5708"/>
       <parent link="forearm"/>
       <child link="leftarm"/>
       <origin rpy="0 0 0" xyz="0 -0.055 1"/>
   </joint>
   
</robot>

